#Error = same length -NaN - Not a Number -dtype -Numpy-0
     


#Functions to get a graph
def getGraph(DataFrame, column1,column2):
    from matplotlib import pyplot as plt
    #check the length of column1 and column2
    if len(DataFrame[column1]) == len(DataFrame[column2]):
        DataFrame.plot(x = column1,y = column2,kind = 'bar',color =['black','pink'])
    else :
          print('please check the column size once')
     

getGraph(df,'home_score','away_score')
